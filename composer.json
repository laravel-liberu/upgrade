{
    "name": "laravel-liberu/upgrade",
    "description": "Helper classes dependency for Laravel Liberu",
    "keywords": [
        "laravel-liberu",
        "upgrade"
    ],
    "homepage": "https://github.com/laravel-liberu/upgrade",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://laravel-liberu.com",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/framework": "^9.0|^10.0",
        "laravel-liberu/core": "^8.0",
        "laravel-liberu/helpers": "^2.0",
        "laravel-liberu/permissions": "^5.0",
        "laravel-liberu/roles": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelLiberu\\Upgrade\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelLiberu\\Upgrade\\AppServiceProvider"
            ],
            "aliases": {}
        }
    },
    "require-dev": {
        "rector/rector": "^0.17.0"
    }
}
